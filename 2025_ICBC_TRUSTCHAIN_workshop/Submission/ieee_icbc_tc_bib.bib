	issn = {0048-7333},
	journal = {Research Policy},
	keywords = {Public procurement, Demand for innovation, Intermediation},
	month = mar,
	number = {2},
	pages = {414--426},
	shorttitle = {Connecting demand and supply},
	title = {Connecting demand and supply: {The} role of intermediation in public procurement of innovation},
	url = {https://www.sciencedirect.com/science/article/pii/S0048733315001638},
