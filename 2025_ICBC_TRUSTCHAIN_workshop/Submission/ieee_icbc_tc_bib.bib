	note = {Conference Name: IEEE Access},
	pages = {1005--1030},
	shorttitle = {{PenChain}},
	title = {{PenChain}: {A} {Blockchain}-{Based} {Platform} for {Penalty}-{Aware} {Service} {Provisioning}},
